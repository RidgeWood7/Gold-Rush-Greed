  m_Script: {fileID: 11500000, guid: a0668d88d404b2c489037092701f1e29, type: 3}
  m_Name: Conductor Intro
  m_EditorClassIdentifier: 
  lines:
  - name: Conductor
    dialogue: Hey Doug, hold up a minute!
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Doug
    dialogue: Whatcha need?
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Conductor
    dialogue: I have an offer for you.
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Doug
    dialogue: Alright, just make it quick. I'm trying to get home before dark.
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Conductor
    dialogue: This should only take a few minutes, I promise.
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Doug
    dialogue: Alright, whaddya need.
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Conductor
    dialogue: "I have a cave on some land I inherited recently, but I can\u2019t
      mine down there without getting lost. Would you be willing to mine for me? "
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Doug
    dialogue: I would, but I'm pretty busy panning for gold these days.
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Conductor
    dialogue: There's gold in the mine, and you can keep whatever you find while
      you're down there.
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Doug
    dialogue: Hmmmm, what's the catch?
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Doug
    dialogue: So I mine coal for you, and keep any gold I find while I'm down there?
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Conductor
    dialogue: Exactly!
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Doug
    dialogue: Alright, when can I start?
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Conductor
    dialogue: Whenever you'd like. Just come to my house with your gear, and I'll
      show you to the cave. If you don't have mining equipment, I think someone in
      town sells some.
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
  - name: Conductor
    dialogue: See you later, Doug!
    icon: {fileID: 3943562803724297591, guid: 494eb83602aeb66438e198be60dae2dd, type: 3}
